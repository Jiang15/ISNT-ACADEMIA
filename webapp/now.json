{
    "version": 2,
    "builds": [
        { "src": "package.json", "use": "@now/static-build" },
        { "src": "/models/*.js", "use": "@now/node" },
        { "src": "server.js", "use": "@now/node-server@canary" }  
    ],
    "routes": [
        { "src": "^/(.*)", "dest": "/index.html" }
      ]
  }